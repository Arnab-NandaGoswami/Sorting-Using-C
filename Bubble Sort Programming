#include<stdio.h>
main(){

	int numberOfElement,i,j,swap,counter,a[numberOfElement];
  
	printf("Enter number of element:\n");
	scanf("%d",&numberOfElement);
  
	printf("Enter elements:\n");
	for(i=0;i<numberOfElement;i++){
		scanf("%d", &a[i]);
	}
  
	for(i=0;i<numberOfElement;i++){
		counter=0;
		for(j=i+1;j<numberOfElement;j++){
			if(a[i]>a[j]){
				swap=a[j];
				a[j]=a[i];
				a[i]=swap;
				counter=1;
			}
		}
		if(counter==0){
			break;
		}
	}
  
	printf("Sorted element is:\n");
	for(i=0;i<numberOfElement;i++){
		printf("%d\n",a[i]);
	}
}
