#include<stdio.h>
main(){
	
	int n,a[n],i,j,min,swap;
	
	printf("Enter the number of elements:\n");
	scanf("%d",&n);
	
	printf("Enter the elements:\n");
	for(i=0;i<n;i++){
		scanf("%d", &a[i]);
	}
	
	for(i=0;i<n-1;i++){
		min = i;
		for(j=i+1;j<n;j++){
			if(a[min]>a[j]){
				min=j;
			}
		}
		if(a[min]!=a[i]){
			swap=a[min];
			a[min]=a[i];
			a[i]=swap;
		}
	}
	
	printf("Sorted element is:\n");
	for(i=0;i<n;i++){
		printf("%d\n",a[i]);
	}
	return 0;
}
